<html>
    <body>
        <script>
            var a=[10,20,30,40,50];
            document.write(a+"<br>");

            document.write(a.length+"<br>");

            a.push(60);
            document.write(a+"<br>");

            a.pop();
            document.write(a+"<br>");

            a.unshift(0);
            document.write(a+"<br>");

            a.shift();
            document.write(a+"<br>");

            a.reverse();
            document.write(a+"<br>");

            let c =a.indexOf(50);
            document.write(c+"<br>");

            let v=a.includes(20);
            document.write(v+"<br>");

            a.sort((x, y) => x - y); 
            document.write(a+"<br>");

            var d =a.slice(1,3);
            document.write(d+"<br>");

             var e=a.join(" - ");
            document.write(e+"<br>");

            var b = [70,80];
            var f=a.concat(b);
            document.write(f+"<br>");

            </script>
    </body>
</html>
