<html>
<head>
<script>
	function InternalJS()
	{
		document.write("Welcome to Internal JavaScript");
	}
</script>
</head>
<body>
<input type = 'button' value = "InternalJS" onclick = "InternalJS();">
<input type = 'button' value = "EnternalJS" onclick = "EnternalJS();">
</body>
</html>

-----EXTERNAL.JS-------
function ExternalJS()
{
	document.write("Welcome to External JavaScript");
}

<html>
<head>
<script src = "external.js">
</script>
</head>
<body>
<input type = 'button' value = "InternalJS" onclick = "InternalJS();">
<input type = 'button' value = "EnternalJS" onclick = "ExternalJS();">
</body>
</html>
